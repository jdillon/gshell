<commands xmlns="http://sonatype.org/gshell-commands/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://sonatype.org/gshell-commands/1.0.0 http://sonatype.org/xsd/gshell-commands-1.0.0.xsd"
          version="1.0.0">

    <command-set id="core">
        <enabled>true</enabled>
        <autoRegisterPriority>10</autoRegisterPriority>
        <command>
            <action>org.sonatype.gshell.commands.pref.ListPreferencesCommand</action>
        </command>
        <command>
            <action>org.sonatype.gshell.commands.pref.SetPreferenceCommand</action>
        </command>
        <command>
            <action>org.sonatype.gshell.commands.pref.GetPreferenceCommand</action>
        </command>
        <command>
            <action>org.sonatype.gshell.commands.pref.UnsetPreferenceCommand</action>
        </command>
        <command>
            <action>org.sonatype.gshell.commands.pref.RemovePreferencesCommand</action>
        </command>
        <command>
            <action>org.sonatype.gshell.commands.pref.ImportPreferencesCommand</action>
        </command>
        <command>
            <action>org.sonatype.gshell.commands.pref.ExportPreferencesCommand</action>
        </command>
    </command-set>

</commands>
